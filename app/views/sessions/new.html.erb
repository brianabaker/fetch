
<h1>Log in</h1>
<% if flash[:error] %>
 <ul>
   <li><%= flash[:error] %></li>
 </ul>
<% end %>

<%= form_tag sessions_path do %>
<%= label_tag "Username" %>
<%= text_field_tag :user_name %><br/>
<%= label_tag :password %>
<%= password_field_tag :password %><br/>
<%= submit_tag "Submit" %>
<% end %>



    <p>New user? <%= link_to "Sign up now!", signup_path %></p>
  </div>
</div>
