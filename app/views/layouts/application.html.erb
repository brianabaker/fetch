            <li><a href="/signup"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
            <span class="glyphicon glyphicon-log-in"></span>

            <li><% if logged_in? %>
            <%= link_to "Logout", sessions_path, method: :destroy %>
            <% else %>
            <%= link_to "Login", login_path %>
            <% end %></li>
          </ul>
        </div>
    </nav>
    <%= yield %>
    </div>
    </div>
  </body>


</html>
