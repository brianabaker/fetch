<!DOCTYPE html>
<html>
  <head>
    <title>Fetch</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
   <div class = "container">
     <div class="jumbotron">
    <%= yield %>
  </body>

<% if logged_in? %>
<%= link_to "Logout", sessions_path, method: :delete %>
<% else %>
<%= link_to "Login", login_path %>
<% end %>
  </div>

</html>
