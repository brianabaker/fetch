        <div class="container-fluid">
          <!-- <div class="navbar-header">
            <a class="navbar-brand" href="#">CuddleBuddies</a>
          </div> -->
          <ul class="nav navbar-nav">
            <li><a href="/pets">All CuddleBuddies</a></li>
            <li><a href="">Your Pets</a></li>
            <li><a href="/users/<%=session[:user_id]%>/pets/new">Add A Pet</a></li>
            <li><a href="#">About</a></li>
          </ul>
